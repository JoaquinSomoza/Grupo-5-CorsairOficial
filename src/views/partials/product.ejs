<article class="article__hover">
    <a href="/products/detail/<%= producto.id %>">
        <div>
            <img class="article__principal" src="/images/<%= producto.img %>" alt="<%= producto.img %>" />
        </div>
        <h3>
            <%= producto.titulo %>
        </h3>
        <div class="article_description">
            <p>$<%= producto.precio%>
            </p>
    </a>
    <button class="btn btn__agregar">Agregar<i class="fas fa-dolly d-inline"></i></button>
    </div>
</article>